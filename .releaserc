{
  "branches": ["master"],
  "plugins": [
    "@semantic-release/commit-analyzer", 
    "@semantic-release/release-notes-generator", 
    "@semantic-release/changelog",
    ["@semantic-release/npm", { "pkgRoot": "./dist" }],
    [
      "@semantic-release/github",
      {
        "assets": [
          { "path": "dist/cjs/index.js", "label": "CommonJS distribution" },
          { "path": "dist/esm/index.js", "label": "ESM distribution" },
          { "path": "dist/index.d.ts", "label": "Types distribution" },
          { "path": "dist/package.json", "label": "Package file" }
        ]
      }
    ]
  ]
}